            </cmp-field>
            <finder>
                <name>findAdministratorsByOrganizationID</name>
                <!-- Custom Finder Method to find users with ADMIN or SUPER roles (Users table is implied in the FROM section of the query) -->
                <query>, USER_ROLES WHERE USER_ROLES.USRS_ID = USERS.ID AND USERS.ORG_ID={0} AND ( (USER_ROLES.ROLES_NAME = 'ADMIN') OR (USER_ROLES.ROLES_NAME = 'SUPER') )</query>
                <order></order>
            </finder>
            <finder>
                <name>findByAlias</name>
                <!-- Custom Finder Method -->
                <query>, ALIASES WHERE ALIASES.USER_ID = USERS.ID AND ALIASES.ALIAS={0}</query>
                <order></order>
            </finder>
            <finder>
                <name>findByDisplayIDOrExtension</name>
                <query>DISPLAY_ID = {0} OR EXTENSION = {0}</query>
                <order></order>
            </finder>
        </entity>

