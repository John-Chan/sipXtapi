            </cmp-field>
            <cmp-field>
                <field-name>content</field-name>
                <column-name>CONTENT</column-name>
            </cmp-field>
            <finder>
                <name>findByDeviceType</name>
                <query>, PHONE_TYPE_RP_ASSOC assoc
                WHERE ID=assoc.REF_PROP_ID
                AND assoc.PT_ID= {0}</query>
                <order></order>
            </finder>
            <finder>
                <name>findByDeviceTypeAndProfileType</name>
                <query>, PHONE_TYPE_RP_ASSOC assoc
                WHERE ID=assoc.REF_PROP_ID
                AND assoc.PT_ID= {0}
                AND PROFILE_TYPE ={1}</query>
                <order></order>
            </finder>
            <finder>
                <name>findByRefConfigurationSetID</name>
                <query>, CS_PROPERTY_PERMISSIONS csrp
                WHERE ID=csrp.REF_PROP_ID
                AND csrp.RCS_ID={0}</query>
                <order></order>
            </finder>
        </entity>

