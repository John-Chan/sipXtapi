                <column-name>CONTENT</column-name>
            </cmp-field>
            <finder>
                <name>findByUserIDAndProfileType</name>
                <!-- Custom Finder Method to find ConfigSets for user -->
                <query>, USER_CS_ASSOC ASSOC WHERE CONFIG_SETS.ID=ASSOC.CS_ID AND ASSOC.USRS_ID={0} AND PROFILE_TYPE = {1}</query>
                <order></order>
            </finder>
            <finder>
                <name>findByLogicalPhoneID</name>
                <!-- Custom Finder Method to find ConfigSets for LogicalPhone -->
                <query>, LOG_PHONE_CS_ASSOC ASSOC WHERE CONFIG_SETS.ID=ASSOC.CS_ID AND ASSOC.LOG_PN_ID={0}</query>
                <order></order>
            </finder>
            <finder>
                <name>findByDeviceGroupID</name>
                <!-- Custom Finder Method to find ConfigSets for DeviceGroup -->
                <query>, PNG_CS_ASSOC ASSOC WHERE CONFIG_SETS.ID=ASSOC.CS_ID AND ASSOC.PG_ID={0}</query>
                <order></order>
            </finder>
            <finder>
                <name>findByUserGroupIDAndProfileType</name>
                <!-- Custom Finder Method to find ConfigSets for UserGroup -->
                <query>, UG_CS_ASSOC ASSOC WHERE CONFIG_SETS.ID=ASSOC.CS_ID AND ASSOC.UG_ID={0} AND PROFILE_TYPE = {1}</query>
                <order></order>
            </finder>
            <finder>
                <name>findByUserID</name>
                <!-- Custom Finder Method to find ConfigSets for user -->
                <query>, USER_CS_ASSOC ASSOC WHERE CONFIG_SETS.ID=ASSOC.CS_ID AND ASSOC.USRS_ID={0}</query>
                <order></order>
            </finder>
            <finder>
                <name>findByUserGroupID</name>
                <!-- Custom Finder Method to find ConfigSets for UserGroup -->
                <query>, UG_CS_ASSOC ASSOC WHERE CONFIG_SETS.ID=ASSOC.CS_ID AND ASSOC.UG_ID={0}</query>
                <order></order>
            </finder>
        </entity>

