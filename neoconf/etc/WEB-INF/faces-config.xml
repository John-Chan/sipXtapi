<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">

<faces-config>
	
	<application>
	</application>

	<navigation-rule>
		<from-view-id>/endpoint-edit.jsf</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/endpoint-list.jsf</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/endpoint-list.jsf</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<managed-bean>
		<managed-bean-name>editEndpoint</managed-bean-name>
		<managed-bean-class>org.sipfoundry.sipxconfig.site.EditEndpoint</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
  		
	<managed-bean>
		<managed-bean-name>endpoint</managed-bean-name>
		<managed-bean-class>org.sipfoundry.sipxconfig.phone.Endpoint</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>network</managed-bean-name>
		<managed-bean-class>org.sipfoundry.sipxconfig.settings.Network</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>dialPlan</managed-bean-name>
		<managed-bean-class>org.sipfoundry.sipxconfig.admin.dialplan.DialPlan</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
    
	<managed-bean>
		<managed-bean-name>dialPlans</managed-bean-name>
		<managed-bean-class>org.sipfoundry.sipxconfig.admin.dialplan.DialPlanList</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

    <validator>
        <validator-id>org.sipfoundry.PhoneNumber</validator-id>
        <validator-class>org.sipfoundry.sipxconfig.admin.dialplan.PhoneNumberValidator</validator-class>
    </validator>

</faces-config>
