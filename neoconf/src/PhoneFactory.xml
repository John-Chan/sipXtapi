<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
  "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="phoneFactory" class="org.sipfoundry.sipxconfig.core.PhoneFactoryImpl">
	</bean>

    <bean id="polycom300" class="org.sipfoundry.sipxconfig.vendor.PolycomPhone" 
		    singleton="false">
        <property name="modelId"><value>polycom300</value></property>
        <property name="sipxConfig"><ref bean="sipxconfig"/></property>		
    </bean>

</beans>
