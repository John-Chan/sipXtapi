<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.sipfoundry.sipxconfig.domain" default-lazy="false">

  <class name="Domain" table="domain" discriminator-value="unmanagedDomain">
    <id name="id" column="domain_id" type="int" unsaved-value="-1">
      <generator class="sequence">
        <param name="sequence">domain_seq</param>
      </generator>
    </id>
    <discriminator column="bean_id"/>
    <property name="name"/>
    <property name="description"/>
    <property name="beanId" insert="false" update="false"/>
    <many-to-one cascade="all" name="valueStorage" column="value_storage_id"
      class="org.sipfoundry.sipxconfig.setting.ValueStorage"/>
  </class>  
  
</hibernate-mapping>
