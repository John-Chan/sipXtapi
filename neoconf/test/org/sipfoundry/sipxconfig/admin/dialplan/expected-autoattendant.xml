<?xml version="1.0"?>
<vxml>
	<form id="aa">
		<field name="menu">
			<filled>
				<if cond="menu=='0'" >
					<assign name="operation" expr="'operator'" />
                    <assign name="extension" expr=""/>
					<goto nextitem="menuaction" />
					<elseif cond="menu=='1'" />
					<assign name="operation" expr="'disconnect'" />
                    <assign name="extension" expr=""/>
					<goto nextitem="menuaction" />
					<else />
					<assign name="action" expr="'transfer'" />
					<assign name="extension" expr="menu" />
					<goto nextitem="transfertoextn"/>
				</if>
			</filled>
		</field>
	</form>
</vxml>