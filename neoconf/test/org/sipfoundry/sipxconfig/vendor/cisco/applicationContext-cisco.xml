<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
  "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

    <bean id="sipxconfig" class="org.sipfoundry.sipxconfig.core.SipxConfig">
		<property name="devicePlugins">
		    <map>
      			<entry key="cisco">
					<ref bean="ciscoDevicePlugin"/>
				</entry>
    		</map>
		</property>
    </bean>
		
	<bean id="ciscoDevicePlugin" class="org.sipfoundry.sipxconfig.vendor.cisco.CiscoDevicePlugin">
	</bean>

</beans>
