<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE page-specification PUBLIC "-//Apache Software Foundation//Tapestry Specification 4.0//EN" "http://jakarta.apache.org/tapestry/dtd/Tapestry_4_0.dtd">
<page-specification>

  <property name="startTime" persist="session" />
  <property name="endTime" persist="session" />
  <property name="cdrSearch" persist="session" />

  <component id="cdrDates" type="cdr/CdrDates">
    <binding name="startTime" value="startTime" />
    <binding name="endTime" value="endTime" />
  </component>

  <component id="cdrStatsPanel" type="cdr/CdrStatsPanel">
    <binding name="startTime" value="startTime" />
    <binding name="endTime" value="endTime" />
    <binding name="cdrSearch" value="cdrSearch" />
  </component>

</page-specification>
