<ref_property code="cs_1055">
 	<!--
 	IP address of the primary SIP proxy server that will be used by the phones.
 	Enter this address in IP dotted-decimal notation.

 	Port number of the primary SIP proxy server. This is the port on which the SIP
 	client listens for messages. The default is 5060.

	Note For additional phone lines, proxyN_address and proxyN_port parameters can be
	used to assign different proxy addresses to different phone lines. "N" in the
	parameters represents a phone line. The value of "N" can be from 2 to 6. If
	the value of "N" is not specified in the proxyN_address parameter, the phone
	uses the proxy1_address parameter as the default.
 	-->
    <definition name="proxy1" mobility="false" cardinality="1"
    	description="Primary SIP proxy server">


		<value name="container" type="container" cardinality="1" visibility="write">
			<value name="proxy1_address" type="string" visibility="write" cardinality="1"
				description="proxy1_address (SIP Proxy server IP address)" />

			<value name="proxy1_port" type="integer" visibility="write" cardinality="1"
                description="proxy1_address_port (SIP Proxy server port)">

				<default_value>5060</default_value>
				<max_value>65536</max_value>
				<min_value>0</min_value>
			</value>
       	</value>

    </definition>

 </ref_property>



 <ref_property code="cs_1086">
 	<!--
 	IP address or DNS name of SIP proxy server that will be used by phone lines other
 	than line 1. For IP address, use the IP dotted-decimal notation. If the proxyN_address
 	parameter is provisioned with an FQDN, the phone sends REGISTER and INVITE messages
 	by using the FQDN in the Req-URI, To, and From fields. If you want to use a dotted
 	IP address, the proxyN_address parameters should be configured as dotted IP addresses.

	Port number of the SIP proxy server that will be used by phone lines other than line 1.
 	-->
    <definition name="proxy2" mobility="false" cardinality="0..1"
    	description="Proxy server used by phone line 2">


		<value name="container" type="container" cardinality="1" visibility="write">
			<value name="proxy2_address" type="string" visibility="write" cardinality="1"
				description="proxy2_address (SIP Proxy server IP address)" />

			<value name="proxy2_port" type="integer" visibility="write" cardinality="0..1"
                description="proxy2_address_port (SIP Proxy server port)">

				<default_value>5060</default_value>
				<max_value>65536</max_value>
				<min_value>0</min_value>
			</value>
       	</value>

    </definition>

 </ref_property>

