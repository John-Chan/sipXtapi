<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
  "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  <bean id="org.sipfoundry.sipxconfig.phone.hitachi.HitachiModel.MODEL_3000"
    class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
  <bean id="org.sipfoundry.sipxconfig.phone.hitachi.HitachiModel.MODEL_5000"
    class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
  <bean id="org.sipfoundry.sipxconfig.phone.hitachi.HitachiModel.MODEL_5000A"
    class="org.springframework.beans.factory.config.FieldRetrievingFactoryBean" />
  <!-- Hitachi phones -->
  <bean id="hitachi" class="org.sipfoundry.sipxconfig.phone.hitachi.HitachiPhone"
    singleton="false" parent="unmanagedPhone">
  </bean>
</beans>
