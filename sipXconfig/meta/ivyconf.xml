<ivyconf>
  <conf defaultResolver="sipXconfig"/>
  <resolvers>
    <dual name="sipXconfig">
      <chain name="ivyreps">
        <ivyrep name="ivyrep-sandbox" ivyroot="http://opensvn.csie.org/ivyrepsandbox/"/>
        <ivyrep name="ivyrep"/>
      </chain>
      <chain name="artifacts">
        <ibiblio name="ibiblio"/>
        <filesystem name="libraries">
          <artifact pattern="${top.dir}/lib/[artifact].[type]"/>
        </filesystem>
      </chain>
    </dual>
  </resolvers>
</ivyconf>