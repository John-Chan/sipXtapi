<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
	"http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

  <bean id="weblib.test.properties"
    class="org.springframework.core.io.ClassPathResource">
    <constructor-arg index="0">
      <value>weblib.test.properties</value>
    </constructor-arg>
  </bean>
  <bean id="enableSystemProperties"
    class="org.springframework.beans.factory.config.PropertyPlaceholderConfigurer">
    <property name="location">
      <ref bean="sipxconfig.properties"></ref>
    </property>
  </bean>
  <bean id="webTestHelper" class="org.sipfoundry.sipxconfig.site.test.WebTestHelper">
    <property name="project" value="weblib"/>
    <property name="testSetupFactory" ref="testSetupFactory"/>
  </bean>
  <bean id="testSetupFactory" class="org.sipfoundry.sipxconfig.site.test.WebTestSetupFactory">
    <property name="buildDirectory" value="${buildDirectory}"/>
    <property name="webAppName" value="${project}"/>
  </bean>
</beans>