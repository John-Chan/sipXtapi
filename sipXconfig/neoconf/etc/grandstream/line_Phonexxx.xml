<?xml version="1.0" ?>
<!DOCTYPE model 
    PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
    "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd" [
  <!ENTITY types SYSTEM "types.xml">
  ]>
<model>
    &types;
    <group name="port">
        <label>Registration</label>
        <setting name="P31-P410-P510-P610">
            <label>Enable SIP Registration</label>
            <type>
                <boolean/>
            </type>
            <value>1</value>
        </setting>
        <setting name="P3-P407-P507-P607">
            <label>User Caller Name</label>
            <type>
                <string/>
            </type>
            <value></value>
        </setting>
        <setting name="P35-P404-P504-P604">
            <label>SIP User ID</label>
            <type>
                <string/>
            </type>
            <value>8000</value>
        </setting>
        <setting name="P36-P405-P505-P605">
            <label>Authentication ID</label>
            <type>
                <string/>
            </type>
            <value>8000</value>
        </setting>
        <setting name="P34-P406-P506-P606">
            <label>Authentication password</label>
            <type>
                <string/>
            </type>
            <value>0000</value>
        </setting>
        <setting name="P33-P426-P526-P626">
            <label>Voice Mail UserID</label>
            <type>
                <string/>
            </type>
            <value></value>
            <description>User ID/extension for voice mail system.</description>
        </setting>
        <setting name="P270-P417-P517-P617" if="PhoneGxp">
            <label>Account Name</label>
            <type>
                <string/>
            </type>
            <value></value>
        </setting>
        <setting name="P271-P401-P501-P601" if="PhoneGxp">
            <label>Account Active</label>
            <type>
                <boolean/>
            </type>
            <value>1</value>
        </setting>
        <setting name="P47-P402-P502-P602">
            <label>SIP Server</label>
            <type>
                <string/>
            </type>
            <value></value>
        </setting>
        <setting name="P103-P408-P508-P608">
            <label>Use DNS SRV</label>
            <type>
                <boolean/>
            </type>
            <value>1</value>
        </setting>
        <setting name="P48-P403-P503-P603">
            <label>Outbound Proxy</label>
            <type>
                <string/>
            </type>
            <value></value>
        </setting>
        <setting name="P63-P409-P509-P609">
            <label>SIP User ID is phone number</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
        </setting>
    </group>
    <group name="opt">
        <label>User options</label>
        <setting name="P104-P423-P523-P623">
            <label>Default Ring Tone</label>
            <type>
                <enum>
                    <option>
                        <label>system</label>
                        <value>0</value>
                    </option>
                    <option>
                        <label>custom 1</label>
                        <value>1</value>
                    </option>
                    <option>
                        <label>custom 2</label>
                        <value>2</value>
                    </option>
                    <option>
                        <label>custom 3</label>
                        <value>3</value>
                    </option>
                </enum>
            </type>
            <value>0</value>
        </setting>
        <setting name="P90-P425-P525-P625">
            <label>Auto Answer</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
        </setting>
        <setting name="P65-P421-P521-P621">
            <label>Send Anonymous</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
            <description>Caller ID will be blocked if checked.</description>
        </setting>
        <setting name="P71" if="PhoneBt">
            <label>Offhook Auto-Dial</label>
            <type>
                <string/>
            </type>
            <value></value>
            <description>User ID/extension to dial automatically when offhook.</description>
        </setting>
    </group>
    <group name="advanced">
        <label>Advanced</label>
        <setting name="P29-P422-P522-P622">
            <label>Early Dial</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
            <description>Check only if proxy supports 484 response.</description>
        </setting>
        <setting name="P52-P414-P514-P614">
            <label>Disable NAT Traversal</label>
            <type>
                <boolean/>
            </type>
            <value>1</value>
        </setting>
        <setting name="P66-P419-P519-P619">
            <label>Dial Plan</label>
            <type>
                <string/>
            </type>
            <value></value>
            <description>Prefix string added to called numbers.</description>
        </setting>
        <setting name="P73-P416-P516-P616">
            <label>Send DTMF</label>
            <type>
                <enum>
                    <option>
                        <label>in audio</label><value>0</value>
                    </option>
                    <option>
                        <label>via RTP</label><value>1</value>
                    </option>
                    <option>
                        <label>via SIP INFO</label><value>2</value>
                    </option>
                </enum>
            </type>
            <value>1</value>
        </setting>
        <setting name="P81-P411-P511-P611">
            <label>Unregister On Reboot</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
        </setting>
        <setting name="P99-P415-P515-P615">
            <label>SUBSCRIBE for MWI</label>
            <type>
                <boolean/>
            </type>
            <value>1</value>
            <description>Whether or not send SUBSCRIBE for Message Waiting Indication.</description>
        </setting>
        <setting name="P191-P420-P520-P620">
            <label>Enable/Disable Call Features</label>
            <type>
                <boolean/>
            </type>
            <value>1</value>
        </setting>
        <setting name="P197-P418-P518-P618">
            <label>Proxy-Require</label>
            <type>
                <string/>
            </type>
            <value></value>
            <description>A SIP extension to enable firewall penetration.</description>
        </setting>
        <setting name="P74" if="PhoneBt">
            <label>Send Flash Event</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
            <description>Flash will be sent as a DTMF event if checked.</description>
        </setting>
        <setting name="P40-P413-P513-P613">
            <label>Local SIP port</label>
            <type>
                <integer min="1024" max="65535"/>
            </type>
            <value></value>
            <description>Local SIP port</description>
        </setting>
        <setting name="P57-P451-P551-P651">
            <label>First codec</label>
            <type refid="gscodec"/>
            <value>0</value>
        </setting>
        <setting name="P58-P452-P552-P652">
            <label>Second codec</label>
            <type refid="gscodec"/>
            <value>2</value>
        </setting>
        <setting name="P59-P453-P553-P653">
            <label>Third codec</label>
            <type refid="gscodec"/>
            <value>4</value>
        </setting>
        <setting name="P60-P454-P554-P654">
            <label>Fourth codec</label>
            <type refid="gscodec"/>
            <value>8</value>
        </setting>
        <setting name="P61-P455-P555-P655">
            <label>Fifth codec</label>
            <type refid="gscodec"/>
            <value>15</value>
        </setting>
        <setting name="P62-P456-P556-P656">
            <label>Sixth codec</label>
            <type refid="gscodec"/>
            <value>18</value>
        </setting>
        <setting name="P46-P457-P557-P657">
            <label>Seventh codec</label>
            <type refid="gscodec"/>
            <value>2</value>
        </setting>
        <setting name="P98-P458-P558-P658">
            <label>Eighth codec</label>
            <type refid="gscodec"/>
            <value>9</value>
        </setting>
        <setting name="P49" if="PhoneBt">
            <label>G723 encoding rate</label>
            <type>
                <enum>
                    <option>
                        <label>6.3kbps</label><value>0</value>
                    </option>
                    <option>
                        <label>5.3kbps</label><value>1</value>
                    </option>
                </enum>
            </type>
            <value>0</value>
        </setting>
        <setting name="P96" if="PhoneBt">
            <label>iLBC payload type</label>
            <type>
                <integer min="96" max="127"/>
            </type>
            <value>97</value>
            <description>iLBC payload type. Between 96 and 127</description>
        </setting>
        <setting name="P97" if="PhoneBt">
            <label>iLBC Frame Size</label>
            <type>
                <enum>
                    <option>
                        <label>20 ms</label><value>0</value>
                    </option>
                    <option>
                        <label>30 ms</label><value>1</value>
                    </option>
                </enum>
            </type>
            <value>0</value>
        </setting>
        <setting name="P32-P412-P512-P612" if="PhoneGxp">
            <label>Register Expiration</label>
            <type>
                <integer min="0" max="65535"/>
            </type>
            <value>60</value>
            <description>Register Expiration in minutes. Default 1 hour, max 45 days</description>
        </setting>
        <setting name="P198-P424-P524-P624" if="PhoneGxp">
            <label>Special Feature</label>
            <type>
                <enum>
                    <option>
                        <label>Standard</label><value>100</value>
                    </option>
                    <option>
                        <label>Nortel MCS</label><value>101</value>
                    </option>
                    <option>
                        <label>Broadsoft</label><value>102</value>
                    </option>
                    <option>
                        <label>Howdy</label><value>103</value>
                    </option>
                    <option>
                        <label>Sonus</label><value>104</value>
                    </option>
                    <option>
                        <label>3COM</label><value>105</value>
                    </option>
                </enum>
            </type>
            <value>100</value>
        </setting>
        <setting name="P260-P434-P534-P634" if="PhoneGxp">
            <label>Session Expiration</label>
            <type>
                <integer/>
            </type>
            <value>180</value>
            <description>sec</description>
        </setting>
        <setting name="P261-P427-P527-P627" if="PhoneGxp">
            <label>Min-SE</label>
            <type>
                <integer min="90"/>
            </type>
            <value>90</value>
            <description>sec, minimum = 90 sec</description>
        </setting>
        <setting name="P262-P428-P528-P628" if="PhoneGxp">
            <label>Caller Request Timer</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
            <description>Request for timer when making outbound calls.</description>
        </setting>
        <setting name="P263-P429-P529-P629" if="PhoneGxp">
            <label>Callee Request Timer</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
            <description>When caller supports timer but did not request one.</description>
        </setting>
        <setting name="P264-P430-P530-P630" if="PhoneGxp">
            <label>Force Timer</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
            <description>User timer even when remote party does not support.</description>
        </setting>
        <setting name="P266-P432-P532-P632" if="PhoneGxp">
            <label>UAC Specify Refresher</label>
            <type>
                <enum>
                    <option>
                        <label>UAC</label><value>1</value>
                    </option>
                    <option>
                        <label>UAS</label><value>2</value>
                    </option>
                    <option>
                        <label>Omit</label><value>0</value>
                    </option>
                </enum>
            </type>
            <value>0</value>
        </setting>
        <setting name="P267-P433-P533-P633" if="PhoneGxp">
            <label>UAS Specify Refresher</label>
            <type>
                <enum>
                    <option>
                        <label>UAC</label><value>1</value>
                    </option>
                    <option>
                        <label>UAS</label><value>2</value>
                    </option>
                </enum>
            </type>
            <value>1</value>
            <description>When UAC did not specify refresher tag.</description>
        </setting>
        <setting name="P265-P431-P531-P631" if="PhoneGxp">
            <label>Force INVITE</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
            <description>Always refresh with INVITE instead of UPDATE.</description>
        </setting>
        <setting name="P272-P435-P535-P635" if="PhoneGxp">
            <label>Enable 100rel</label>
            <type>
                <boolean/>
            </type>
            <value>0</value>
        </setting>
    </group>
</model>

