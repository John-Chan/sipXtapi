<?xml version="1.0" ?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd" [
  <!ENTITY types SYSTEM "types.xml">
  ]>
<model>
  &types;
  <group name="codec">
    <setting name="P98">
      <label>Eighth codec</label>
      <type refid="gscodec"/>
      <value>9</value>
    </setting>
  </group>
  <group name="budgetone">
    <label>BudgeTone Specific</label>
    <setting name="P90">
      <label>Auto Answer</label>
      <type>
        <boolean/>
      </type>
      <value>0</value>
      <description>Auto Answer</description>
    </setting>
    <setting name="P102">
      <label>Date Display Format</label>
      <type>
        <enum>
          <option>
            <label>Y-M-D</label>
            <value>0</value>
          </option>
          <option>
            <label>M-D-Y</label>
            <value>1</value>
          </option>
          <option>
            <label>D-M-Y</label>
            <value>2</value>
          </option>
        </enum>
      </type>
      <value>0</value>
    </setting>
    <setting name="P104">
      <label>Default Ring Tone</label>
      <type>
        <enum>
          <option>
            <label>system</label>
            <value>0</value>
          </option>
          <option>
            <label>custom 1</label>
            <value>1</value>
          </option>
          <option>
            <label>custom 2</label>
            <value>2</value>
          </option>
          <option>
            <label>custom 3</label>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value>0</value>
    </setting>
    <setting name="P105">
      <label>Caller ID for ring tone 1</label>
      <type>
        <string/>
      </type>
      <value>0</value>
      <description>Use custom ring tone 1 if incoming caller ID is the following.</description>
    </setting>
    <setting name="P106">
      <label>Caller ID for ring tone 2</label>
      <type>
        <string/>
      </type>
      <value>0</value>
      <description>Use custom ring tone 2 if incoming caller ID is the following.</description>
    </setting>
    <setting name="P107">
      <label>Caller ID for ring tone 3</label>
      <type>
        <string/>
      </type>
      <value>0</value>
      <description>Use custom ring tone 3 if incoming caller ID is the following.</description>
    </setting>
  </group>
</model>