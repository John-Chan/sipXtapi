# used in applicationContext-sipxconfig.xml to set system properies
sip.proxyHost=${PROXY_SERVER_ADDR}
sip.proxyPort=${PROXY_SERVER_SIP_PORT}
phoneDefaults.outboundProxy=${SIPXCHANGE_DOMAIN_NAME}
phoneDefaults.outboundProxyPort=5060
# proxy will forward to registry server.
phoneDefaults.registrationServer=${SIPXCHANGE_DOMAIN_NAME}
phoneDefaults.registrationServerPort=5060
phoneDefaults.tftpServer=${CONFIG_SERVER_ADDR}
phoneDefaults.authorizationRealm=${SIPXCHANGE_REALM}
phoneDefaults.domainName=${SIPXCHANGE_DOMAIN_NAME}
sipxServer.domainName=${SIPXCHANGE_DOMAIN_NAME}
# imdb/sipdb location for https access
registrationContext.server=${MY_FULL_HOSTNAME}
registrationContext.port=${CONFIG_SERVER_HTTPS_PORT}
coreContextImpl.authorizationRealm=${SIPXCHANGE_REALM}
coreContextImpl.domainName=${SIPXCHANGE_DOMAIN_NAME}
adminContextImpl.backupDirectory=@sipxpbx.backup.dir@
dataSource.url=jdbc:postgresql://localhost/SIPXCONFIG
vxml.promptsDirectory=@sipxvxml.data.dir@/prompts
vxml.scriptsDirectory=@www.dir@/doc/aa_vxml
orbitsGenerator.audioDirectory=@sipxpark.audio.dir@

# external aliases
externalAliases.addinsDirectory=@sipxpbx.data.dir@
# it should be set to dolar_sign{dataset.alias.addins} for backward compatibility but configpp returns errors
externalAliases.aliasAddins=

# name changes to anything starting w/sysdir need to be reflected in
# VersionInfo class as well
sysdir.bin=@sipxpbx.bin.dir@
sysdir.etc=@sipxpbx.conf.dir@
sysdir.log=@sipxpbx.log.dir@
sysdir.share=@sipxpbx.data.dir@
sysdir.phone=@sipxconfig.phone.dir@
sysdir.tmp=@sipxpbx.tmp.dir@
sysdir.version=@version@
sysdir.build_number=0000
sysdir.comment=opendev
