phoneDefaults.outboundProxy=${PROXY_SERVER_ADDR}
phoneDefaults.outboundProxyPort=${PROXY_SERVER_SIP_PORT}
# proxy will forward to registry server.
phoneDefaults.registrationServer=${PROXY_SERVER_ADDR}
phoneDefaults.registrationServerPort=${PROXY_SERVER_SIP_PORT}
phoneDefaults.tftpServer=${CONFIG_SERVER_ADDR}
phoneDefaults.authorizationRealm=${SIPXCHANGE_REALM}
