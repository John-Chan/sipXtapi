<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd">
<model>
  <group name="port">
    <label>Port Configuration</label>
    <setting name="UID">
      <type>
        <string required="yes"/>
      </type>
      <value></value>
      <description>User ID</description>
    </setting>
    <setting name="PWD">
      <type>
        <string password="yes"/>
      </type>
      <value></value>
      <description>Password</description>
    </setting>
    <setting name="DisplayName">
      <value></value>
      <description>Display name for Caller ID</description>
    </setting>
    <setting name="LoginID">
      <value></value>
      <description>Authentication ID if device setting UseLoginID = 1</description>
    </setting>
    <setting name="ShortName" if="79.*">
      <value>0</value>
      <description>Display name for LCD screen</description>
    </setting>
  </group>
</model>
