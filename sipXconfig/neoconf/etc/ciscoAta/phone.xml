      <description>Timezone.</description>
    </setting>
    <setting name="IPDialPlan">
      <type>
        <enum>
          <option>
            <label>Disable IP dialing</label>
            <value>0</value>
          </option>
          <option>
            <label>IP dialing if 2x. seen</label>
            <value>1</value>
          </option>
          <option>
            <label>IP dialing if 3x. seen</label>
            <value>2</value>
          </option>
        </enum>
      </type>
      <value>1</value>
      <description>Allow IP-like destination address in DialPlan.</description>
    </setting>
    <setting name="DialPlan">
      <type>
        <string maxLen="199"/>
      </type>
      <value>*St4-|#St4-|911|1&gt;#t8.r9t2-|0&gt;#t811.rat4-|^1t4&gt;#.-</value>
      <description>The programmable dial plan (see ATA administration guide).</description>
    </setting>
    <setting name="DialPlanEx">
      <type>
        <string maxLen="499"/>
      </type>
      <value>0</value>
      <description>The programmable dial plan (see ATA administration guide).</description>
    </setting>
    <setting name="ACRDN">
      <type>
        <string maxLen="8"/>
      </type>
      <value>0</value>
      <description>Anonymous call rejection directory number.</description>
    </setting>
  </group>
  <group name="debug">
    <label>Debug Parameters</label>
    <setting name="NPrintf">
      <type>
        <!-- extended IP address -->
      </type>
      <value>0</value>
      <description>Set host for debugging messages.</description>
    </setting>
    <setting name="SyslogIP">
      <type>
        <!-- extended IP address -->
      </type>
      <value>0.0.0.0.514</value>
      <description>Syslog server address.</description>
    </setting>
    <setting name="SyslogCtrl">
      <type refid="bitmap"/>
      <value>0x00000000</value>
      <description>Turn on specific syslog traces.</description>
    </setting>
    <group name="traceflagsgroup">
      <label>TraceFlags</label>
      <setting name="TraceFlags" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Set trace features for diagnostic use.</description>
      </setting>
      <setting name="__TraceFlags.0" advanced="yes">
        <label>SIP Debug Level</label>
        <type>
          <integer min="0" max="3"/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__TraceFlags.8" advanced="yes">
        <label>RTP Statistics Log</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
    </group>
  </group>
</model>