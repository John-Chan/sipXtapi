<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd" [
  <!ENTITY types SYSTEM "types.xml">
  ]>
<model>
  &types;
  <group name="network">
    <group name="opflagsgroup">
      <label>OpFlags</label>
      <setting name="OpFlags" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Various operational features.</description>
      </setting>
      <setting name="__OpFlags.0" advanced="yes">
        <label>TFTP configuration filename</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Use only internal default TFTP configuration filename.</description>
      </setting>
      <setting name="__OpFlags.1" advanced="yes">
        <label>Network probing on power up</label>
        <type>
          <boolean/>
        </type>
        <value>1</value>
        <description>Do not perform static network router probing at power up.</description>
      </setting>
      <setting name="__OpFlags.3" advanced="yes">
        <label>DHCP option 150</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not ask for DHCP option 150 in DHCP DISCOVERY message.</description>
      </setting>
      <setting name="__OpFlags.4" advanced="yes">
        <label>VLAN network operation</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>User the static VLAN ID specified in VLANSetting.</description>
      </setting>
      <setting name="__OpFlags.5" advanced="yes">
        <label>VLAN encapsulation</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not use VLAN IP encapsulation.</description>
      </setting>
      <setting name="__OpFlags.6" advanced="yes">
        <label>Cisco Discovery Protocol</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not perform CDP discovery.</description>
      </setting>
      <setting name="__OpFlags.7" advanced="yes">
        <label>Web configuration access</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not allow web configuration.</description>
      </setting>
      <setting name="__OpFlags.8" advanced="yes">
        <label>HTTP refresh access</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not allow http://ip/refresh.</description>
      </setting>
      <setting name="__OpFlags.9" advanced="yes">
        <label>HTTP reset access</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not allow reset via http://ip/reset.</description>
      </setting>
      <setting name="__OpFlags.15" advanced="yes">
        <label>Disable UDP checksum</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Disable generation of UDP checksum in outgoing UDP packets.</description>
      </setting>
    </group>
    <group name="nattimergroup">
      <label>NatTimer</label>
      <setting name="NatTimer" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Operational parameters for NAT.</description>
      </setting>
      <setting name="__NatTimer.0" advanced="yes">
        <label>SIP/RTP keep alive period</label>
        <type>
          <integer min="0" max="2047"/>
        </type>
        <value>0</value>
        <description>To keep NAT/firewall port bindings open (seconds).</description>
      </setting>
      <setting name="__NatTimer.18" advanced="yes">
        <label>SIP keep alive destination</label>
        <type>
          <enum>
            <option>
              <label>NatServer</label>
              <value>0</value>
            </option>
            <option>
              <label>ProxyServer</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__NatTimer.20" advanced="yes">
        <label>Keep alive for SIPPort</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>To either NatServer or Proxy.</description>
      </setting>
      <setting name="__NatTimer.21" advanced="yes">
        <label>Keep alive for MediaPort</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>To NatServer.</description>
      </setting>
      <setting name="__NatTimer.22" advanced="yes">
        <label>Keep alive for MediaPort+4</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>To NatServer.</description>
      </setting>
    </group>
  </group>
  <group name="audio">
    <label>Audio Parameters</label>
    <setting name="RxCodec">
      <type>
        <enum>
          <option>
            <label>G.711a</label>
            <value>1</value>
          </option>
          <option>
            <label>G.711u</label>
            <value>2</value>
          </option>
          <option>
            <label>G.729a</label>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value>2</value>
      <description>Receiving-audio codec preference.</description>
    </setting>
    <setting name="TxCodec">
      <type>
        <enum>
          <option>
            <label>G.711a</label>
            <value>1</value>
          </option>
          <option>
            <label>G.711u</label>
            <value>2</value>
          </option>
          <option>
            <label>G.729a</label>
            <value>3</value>
          </option>
        </enum>
      </type>
      <value>2</value>
      <description>Transmitting-audio codec preference.</description>
    </setting>
    <setting name="NumTxFrames">
      <type>
        <integer min="1" max="6"/>
      </type>
      <value>2</value>
      <description>Default RTP packet size in frames per packet.</description>
    </setting>
    <group name="audiomodegroup">
      <label>AudioMode</label>
      <setting name="AudioMode" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Audio operating mode.</description>
      </setting>
      <setting name="__AudioMode.0">
        <label>Enable Silence Suppression</label>
        <type>
          <boolean/>
        </type>
        <value>1</value>
      </setting>
      <setting name="__AudioMode.4">
        <label>DTMF Transmission Method</label>
        <type>
          <enum>
            <option>
              <label>Always inband</label>
              <value>0</value>
            </option>
            <option>
              <label>Negotiated via SDP</label>
              <value>1</value>
            </option>
            <option>
              <label>Always out-of-band</label>
              <value>2</value>
            </option>
          </enum>
        </type>
        <value>1</value>
      </setting>
    </group>
  </group>
  <group name="country">
    <label>Country settings</label>
    <setting name="_country">
      <label>Country</label>
      <type>
        <enum>
          <option>
            <label>United States</label>
            <value>us</value>
          </option>
          <option>
            <label>Finland</label>
            <value>fin</value>
          </option>
        </enum>
      </type>
      <value>us</value>
      <description>Country specific tone settings</description>
    </setting>
    <group name="_tone.us">
      <label>Tone Parameters - United States</label>
      <setting name="DialTone" advanced="yes">
        <value>2,31538,814,30831,2032,0,0,0,0,0,0</value>
        <description>Dial tone format (see ATA administration guide).</description>
      </setting>
      <setting name="DialTone2" advanced="yes">
        <value>2,30743,1384,29864,1252,0,0,0,0,0,0</value>
        <description>Alert tone format (see ATA administration guide).</description>
      </setting>
      <setting name="BusyTone" advanced="yes">
        <value>2,30467,1104,28959,1404,1,4000,4000,0,0,0</value>
        <description>Busy tone format (see ATA administration guide).</description>
      </setting>
      <setting name="ReorderTone" advanced="yes">
        <value>0,2,30467,1104,28959,1404,0,0,1,2000,2000,0,0,0,0,0,0</value>
        <description>Reorder tone format (see ATA administration guide).</description>
      </setting>
      <setting name="RingBackTone" advanced="yes">
        <value>2,30831,2032,30467,1104,1,16000,32000,0,0,0</value>
        <description>Ring-back tone format (see ATA administration guide).</description>
      </setting>
      <setting name="CallWaitTone" advanced="yes">
        <value>1,30831,2412,0,0,1,2400,2400,0,0,4800</value>
        <description>Call-waiting tone format (see ATA administration guide).</description>
      </setting>
      <setting name="RingOnOffTime" advanced="yes">
        <value>2,4,25</value>
        <description>Ringer cadence pattern (see ATA administration guide).</description>
      </setting>
    </group>
    <group name="_tone.fin">
      <label>Tone Parameters - Finland</label>
      <setting name="DialTone" advanced="yes">
        <value>1,30958,4952,0,0,0,0,0,0,0,0</value>
        <description>Dial tone format (see ATA administration guide).</description>
      </setting>
      <setting name="DialTone2" advanced="yes">
        <value>1,30958,4952,0,0,0,0,0,0,0,0</value>
        <description>Dial tone format (see ATA administration guide).</description>
      </setting>
      <setting name="BusyTone" advanced="yes">
        <value>1,30958,4952,0,0,1,2400,2400,0,0,0</value>
        <description>Busy tone format (see ATA administration guide).</description>
      </setting>
      <setting name="ReorderTone" advanced="yes">
        <value>1,1,30958,5556,0,0,0,0,1,1600,2000,0,0,0,0,0,0</value>
        <description>Reorder tone format (see ATA administration guide).</description>
      </setting>
      <setting name="RingBackTone" advanced="yes">
        <value>1,30958,9545,0,0,1,8000,32000,0,0,0</value>
        <description>Ring-back tone format (see ATA administration guide).</description>
      </setting>
      <setting name="CallWaitTone" advanced="yes">
        <value>1,30831,5493,0,0,2,2400,2400,4800,0,9600</value>
        <description>Call-waiting tone format (see ATA administration guide).</description>
      </setting>
      <setting name="RingOnOffTime" advanced="yes">
        <value>1,5,25</value>
        <description>Ringer cadence pattern (see ATA administration guide).</description>
      </setting>
    </group>
  </group>
  <group name="service">
    <group name="connectmodegroup">
      <label>ConnectMode</label>
      <setting name="ConnectMode" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Call signaling protocol control</description>
      </setting>
      <setting name="__ConnectMode.0" advanced="yes">
        <label>Send INVITE requests</label>
        <type>
          <enum>
            <option>
              <label>via local proxy</label>
              <value>0</value>
            </option>
            <option>
              <label>via header specified URL</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.4" advanced="yes">
        <label>Include instance=MAC-... in Contact:</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.16" advanced="yes">
        <label>SIP registration mode</label>
        <type>
          <enum>
            <option>
              <label>Register without removing previous registrations</label>
              <value>0</value>
            </option>
            <option>
              <label>Remove previous registrations</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
        <description>Default is to send via local proxy.</description>
      </setting>
      <setting name="__ConnectMode.19" advanced="yes">
        <label>Generate ringback tone to the caller</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.20" advanced="yes">
        <label>SIP REGISTER includes action=proxy</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.21" advanced="yes">
        <label>SIP REGISTER includes action=redirect</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.22" advanced="yes">
        <label>Process via header received= tag</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.24" advanced="yes">
        <label>Include RTP statistics in BYE</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
    </group>
    <group name="sigtimergroup">
      <label>SigTimer</label>
      <setting name="SigTimer" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Various timeout values</description>
      </setting>
      <setting name="__SigTimer.0" advanced="yes">
        <label>Period between call waiting tones</label>
        <type>
          <integer min="0" max="255"/>
        </type>
        <value>100</value>
        <description>0.1 second increment, range 0-255 = 0-25.5 sec.</description>
      </setting>
      <setting name="__SigTimer.14" advanced="yes">
        <label>Ring timeout for incoming calls</label>
        <type>
          <integer min="0" max="63"/>
        </type>
        <value>6</value>
        <description>0 = ring forever, 10 seconds increment, range 1-63 = 10-630 
          sec.</description>
      </setting>
      <setting name="__SigTimer.20" advanced="yes">
        <label>No answer timeout before forwarding</label>
        <type>
          <integer min="0" max="63"/>
        </type>
        <value>20</value>
        <description>1 second increment, range 0-63 sec.</description>
      </setting>
      <setting name="__SigTimer.28" advanced="yes">
        <label>First key repeat interval</label>
        <type>
          <integer min="0" max="3"/>
        </type>
        <value>0</value>
        <description>0 = 1 sec, 1 = disable repeat, 2 = 2 sec, 3 = 3 sec</description>
      </setting>
      <setting name="__SigTimer.30" advanced="yes">
        <label>Subsequent key repeat interval</label>
        <type>
          <integer min="0" max="3"/>
        </type>
        <value>0</value>
        <description>0 = 0.25 sec, 1 = 0.5 sec, 2 = 0.75 sec, 3 = 1 sec</description>
      </setting>
    </group>
  </group>
  <group name="upgrade">
    <label>Software Upgrade</label>
    <setting name="logofile">
      <label>Logo filename</label>
      <value>none</value>
      <description>Name of logo file in TFTP directory.</description>
    </setting>
    <setting name="logoid">
      <label>Logo ID</label>
      <value>0</value>
      <description>Logo image version number.</description>
    </setting>
  </group>
  <group name="caller">
    <label>Caller Preferences</label>
    <setting name="ServicesUrl">
      <value>0</value>
      <description>URL to contact for XML Services.</description>
    </setting>
    <setting name="DirectoriesUrl">
      <value>0</value>
      <description>URL to contact for Directory Services.</description>
    </setting>
    <setting name="CallForwardNumber">
      <value>0</value>
      <description>Phone number to forward all calls.</description>
    </setting>
    <setting name="VoiceMailNumber">
      <value>0</value>
      <description>Phone number to access voice mail.</description>
    </setting>
    <setting name="TimeFormat">
      <type>
        <enum>
          <option>
            <label>12h</label>
            <value>h:ia</value>
          </option>
          <option>
            <label>24h</label>
            <value>H:i</value>
          </option>
          <option>
            <label>not displayed</label>
            <value>0</value>
          </option>
        </enum>
      </type>
      <value>h:ia</value>
      <description>Time display format on LCD screen.</description>
    </setting>
    <setting name="DateFormat">
      <type>
        <enum>
          <option>
            <label>mm-dd-yy</label>
            <value>m-d-y</value>
          </option>
          <option>
            <label>mm-dd-yyyy</label>
            <value>m-d-Y</value>
          </option>
          <option>
            <label>dd-mm-yy</label>
            <value>d-m-y</value>
          </option>
          <option>
            <label>dd-mm-yyyy</label>
            <value>d-m-Y</value>
          </option>
          <option>
            <label>dd.mm.yy</label>
            <value>d.m.y</value>
          </option>
          <option>
            <label>dd.mm.yyyy</label>
            <value>d.m.Y</value>
          </option>
          <option>
            <label>yy/mm/dd</label>
            <value>y/m/d</value>
          </option>
          <option>
            <label>yyyy/mm/dd</label>
            <value>Y/m/d</value>
          </option>
          <option>
            <label>yy/mm/dd</label>
            <value>y/m/d</value>
          </option>
          <option>
            <label>yyyy/mm/dd</label>
            <value>Y/m/d</value>
          </option>
          <option>
            <label>Mmm d, yyyy</label>
            <value>M d, Y</value>
          </option>
          <option>
            <label>Mmm d, yy</label>
            <value>M d, y</value>
          </option>
          <option>
            <label>not displayed</label>
            <value>0</value>
          </option>
        </enum>
      </type>
      <value>m-d-y</value>
      <description>Date display format on LCD screen.</description>
    </setting>
    <setting name="DoNotDisturb">
      <type>
        <boolean/>
      </type>
      <value>0</value>
      <description>Activate Do Not Disturb feature.</description>
    </setting>
    <setting name="BlockCallerId">
      <type>
        <boolean/>
      </type>
      <value>0</value>
      <description>Enable blocking of outgoing Caller ID.</description>
    </setting>
    <setting name="CallWaiting">
      <type>
        <boolean/>
      </type>
      <value>0</value>
      <description>Enable call waiting for every call.</description>
    </setting>
    <setting name="AttendedTransfer">
      <type>
        <boolean/>
      </type>
      <value>1</value>
      <description>Enable attended call transfer.</description>
    </setting>
    <setting name="BlindTransfer">
      <type>
        <boolean/>
      </type>
      <value>1</value>
      <description>Enable blind call transfer.</description>
    </setting>
    <setting name="Conference">
      <type>
        <boolean/>
      </type>
      <value>1</value>
      <description>Enable 3-way conference.</description>
    </setting>
    <setting name="BlockAnonymous">
      <type>
        <boolean/>
      </type>
      <value>0</value>
      <description>Activate blocking of anonymous incoming call.</description>
    </setting>
    <setting name="AutoLookUp">
      <type>
        <boolean/>
      </type>
      <value>0</value>
      <description>Enable automatic phone number lookup when dialing.</description>
    </setting>
    <setting name="ForwardToVMDelay">
      <type>
        <integer/>
      </type>
      <value>20</value>
      <description>Number of seconds before forwarding to VoiceMailNumber.</description>
    </setting>
    <setting name="CallPrefGuiShow" advanced="yes">
      <type refid="bitmap"/>
      <value>0xffffffff</value>
      <description>Control whether a call preference option is displayed.</description>
    </setting>
    <setting name="CallPrefGuiSet" advanced="yes">
      <type refid="bitmap"/>
      <value>0xffffffff</value>
      <description>Control whether a call preference option can be set by user.</description>
    </setting>
  </group>
</model>