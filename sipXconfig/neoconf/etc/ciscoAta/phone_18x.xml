  <group name="audio">
    <label>Audio Parameters</label>
    <setting name="RxCodec">
      <type>
        <enum>
          <option>
            <label>G.723.1</label>
            <value>0</value>
          </option>
          <option>
            <label>G.711a</label>
            <value>1</value>
          </option>
          <option>
            <label>G.711u</label>
            <value>2</value>
          </option>
          <option>
            <label>G.729a</label>
            <value>3</value>
          </option>
          <option>
            <label>G.726-32kbps</label>
            <value>6</value>
          </option>
        </enum>
      </type>
      <value>2</value>
      <description>Receiving-audio codec preference.</description>
    </setting>
    <setting name="TxCodec">
      <type>
        <enum>
          <option>
            <label>G.723.1</label>
            <value>0</value>
          </option>
          <option>
            <label>G.711a</label>
            <value>1</value>
          </option>
          <option>
            <label>G.711u</label>
