<?xml version="1.0"?>
<!DOCTYPE model PUBLIC "-//SIPFoundry//sipXconfig//Model specification 2.0//EN" 
  "http://www.sipfoundry.org/sipXconfig/dtd/setting_2_0.dtd" [
  <!ENTITY types SYSTEM "types.xml">
  ]>
<model>
  &types;
  <group name="network">
    <group name="opflagsgroup">
      <label>OpFlags</label>
      <setting name="OpFlags" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Various operational features.</description>
      </setting>
      <setting name="__OpFlags.0" advanced="yes">
        <label>TFTP configuration filename</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Use only internal default TFTP configuration filename.</description>
      </setting>
      <setting name="__OpFlags.1" advanced="yes">
        <label>Network probing on power up</label>
        <type>
          <boolean/>
        </type>
        <value>1</value>
        <description>Do not perform static network routere probing at power up.</description>
      </setting>
      <setting name="__OpFlags.3" advanced="yes">
        <label>DHCP option 150</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not ask for DHCP option 150 in DHCP DISCOVERY message.</description>
      </setting>
      <setting name="__OpFlags.4" advanced="yes">
        <label>VLAN network operation</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>User the static VLAN ID specified in VLANSetting.</description>
      </setting>
      <setting name="__OpFlags.5" advanced="yes">
        <label>VLAN encapsulation</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not use VLAN IP encapsulation.</description>
      </setting>
      <setting name="__OpFlags.6" advanced="yes">
        <label>Cisco Discovery Protocol</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not perform CDP discovery.</description>
      </setting>
      <setting name="__OpFlags.7" advanced="yes">
        <label>Web configuration access</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not allow web configuration.</description>
      </setting>
      <setting name="__OpFlags.8" advanced="yes">
        <label>HTTP refresh access</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not allow http://ip/refresh.</description>
      </setting>
      <setting name="__OpFlags.9" advanced="yes">
        <label>HTTP reset access</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Do not allow reset via http://ip/reset.</description>
      </setting>
      <setting name="__OpFlags.10" advanced="yes">
        <label>Hostname of device</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>In DHCP packets use device's name as hostname (DHCP option 
          12).</description>
      </setting>
      <setting name="__OpFlags.13" advanced="yes">
        <label>DNS servers 1</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Use both DHCP provided (preferred) and statically configured DNS 
          IPs.</description>
      </setting>
      <setting name="__OpFlags.14" advanced="yes">
        <label>DNS servers 2</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Use both statically configured (preferred) and DHCP provided DNS 
          IPs.</description>
      </setting>
      <setting name="__OpFlags.15" advanced="yes">
        <label>Disable UDP checksum</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>Disable generation of UDP checksum in outgoing UDP packets.</description>
      </setting>
      <setting name="__OpFlags.28" advanced="yes">
        <label>Password protection</label>
        <type>
          <boolean>
            <true>
              <value>6</value>
            </true>
            <false>
              <value>0</value>
            </false>
          </boolean>
        </type>
        <value>0</value>
        <description>Factory Reset and Local Upgrade protected by UIPassword also.</description>
      </setting>
    </group>
    <group name="nattimergroup">
      <label>NatTimer</label>
      <setting name="NatTimer" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Operational parameters for NAT.</description>
      </setting>
      <setting name="__NatTimer.0" advanced="yes">
        <label>SIP/RTP keep-alive period</label>
        <type>
          <integer min="0" max="2047"/>
        </type>
        <value>0</value>
        <description>To keep NAT/firewall port bindings open (seconds).</description>
      </setting>
      <setting name="__NatTimer.16" advanced="yes">
        <label>NAT traversal mode</label>
        <type>
          <enum>
            <option>
              <label>Send keep-alive UDP only</label>
              <value>0</value>
            </option>
            <option>
              <label>STUN</label>
              <value>1</value>
            </option>
            <option>
              <label>NATIP auto-mapping</label>
              <value>2</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__NatTimer.18" advanced="yes">
        <label>SIP keep-alive destination</label>
        <type>
          <enum>
            <option>
              <label>NatServer</label>
              <value>0</value>
            </option>
            <option>
              <label>ProxyServer</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__NatTimer.20" advanced="yes">
        <label>Keep-alive for SIPPort</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>To either NatServer or Proxy.</description>
      </setting>
      <setting name="__NatTimer.21" advanced="yes">
        <label>Keep alive for MediaPort</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>To NatServer.</description>
      </setting>
      <setting name="__NatTimer.22" advanced="yes">
        <label>Keep-alive for MediaPort+4</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
        <description>To NatServer.</description>
      </setting>
    </group>
  </group>
  <group name="sip">
    <group name="sessiontimergroup">
      <label>SessionTimer</label>
      <setting name="SessionTimer" hidden="yes">
        <type refid="bitmap"/>
        <value>0x00000000</value>
        <description>Configure SIP session timer)</description>
      </setting>
      <setting name="__SessionTimer.0" advanced="yes">
        <label>Advertise session timer support</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__SessionTimer.1" advanced="yes">
        <label>Request a session timer on every call</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__SessionTimer.2" advanced="yes">
        <label>Callee should initiate session refresh</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__SessionTimer.4" advanced="yes">
        <label>Enable session debug messages</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
    </group>
  </group>
