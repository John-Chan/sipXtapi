      <setting name="__ConnectMode.20" advanced="yes">
        <label>SIP REGISTER includes action=proxy</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.21" advanced="yes">
        <label>SIP REGISTER includes action=redirect</label>
        <type>
          <boolean/>
