          <enum>
            <option>
              <label>G.711U</label>
              <value>0</value>
            </option>
            <option>
              <label>G.711An</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.14" advanced="yes">
