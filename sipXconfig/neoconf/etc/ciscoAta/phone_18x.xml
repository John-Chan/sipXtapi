    <group name="connectmodegroup">
      <label>ConnectMode</label>
      <setting name="ConnectMode" hidden="yes">
        <type refid="bitmap"/>
        <value>0x0</value>
        <description>Call signaling protocol control</description>
      </setting>
      <setting name="__ConnectMode.0" advanced="yes">
        <label>Where to send INVITE requests</label>
        <type>
          <enum>
            <option>
              <label>INVITE is sent via local proxy</label>
              <value>0</value>
            </option>
            <option>
              <label>INVITE is sent via specified URL</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.1" advanced="yes">
        <label>Remove previous registration mode</label>
        <type>
          <enum>
            <option>
              <label>Remove all registrations using Contact: *</label>
              <value>0</value>
            </option>
            <option>
              <label>Remove last registration only</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.2" advanced="yes">
        <label>G.711 payload type in fax passthru mode</label>
        <type>
          <enum>
            <option>
              <label>G.711U = 126, G.711A = 127</label>
              <value>0</value>
            </option>
            <option>
              <label>G.711U = 0, G.711A = 8</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.3" advanced="yes">
        <label>Include RTP statistics in BYE</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.4" advanced="yes">
        <label>Include instance=MAC-... in Contact:</label>
        <type>
          <boolean/>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.5" advanced="yes">
        <label>Allow INVITE from</label>
        <type>
          <enum>
            <option>
              <label>any SIP UA</label>
              <value>0</value>
            </option>
            <option>
              <label>configured proxies only</label>
              <value>1</value>
            </option>
          </enum>
        </type>
        <value>0</value>
      </setting>
      <setting name="__ConnectMode.7" advanced="yes">
