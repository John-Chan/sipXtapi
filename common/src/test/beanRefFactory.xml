<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" 
  "http://www.springframework.org/dtd/spring-beans.dtd">

<beans> 

   <!-- Alias to profilepublishers real context -->
   <bean id="unittest" class="java.lang.String">
     <constructor-arg>
       <value>org.sipfoundry.sipxconfig.core</value>
     </constructor-arg>
   </bean>
	
   <bean id="org.sipfoundry.sipxconfig.core"
         class="org.springframework.context.support.ClassPathXmlApplicationContext">
     <constructor-arg>
       <list>
         <value>org/sipfoundry/sipxconfig/core/applicationContext-core.xml</value>
       </list>
     </constructor-arg>
   </bean>
   
</beans>
