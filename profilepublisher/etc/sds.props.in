# These values need to be changed on a per install basis
sds.dbURL=jdbc:postgresql://localhost/SDS

# set this to the current db userid
sds.dbUserID=postgres

# set this to the current db password
sds.dbPassword=

# set this to the dbms jdbc driver
sds.dbDriverName=org.postgresql.Driver

# This is the application sequence (bump this up to force
# new app downloads for all subscribing phones)
x-xpressa-apps=1

# This is the Install Script (disable this initially -1
# disables these from going to the phone
x-xpressa-install=-1

