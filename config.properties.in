
# G E N E R A L
version=@VERSION@
top.dir=@abs_srcdir@
java=@JAVA@
java.home=@JAVA_HOME@
optimize=@JAVAC_OPTIMIZED@
deprecation=false
debug=@JAVAC_DEBUG@
